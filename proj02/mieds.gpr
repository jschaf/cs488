project MIEDS is

   for Main use ("mieds.adb");

   package Builder is
      for Default_Switches ("ada") use ("-g");
   end Builder;

   package Compiler is
      for Default_Switches ("ada") use ("-g", "-fstack-check", "-gnatf", "-gnata");
   end Compiler;

   for Object_Dir use "build";
   for Exec_Dir use ".";

end MIEDS;

